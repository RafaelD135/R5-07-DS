Debein Rafael
public class Q4 {
    public static void afficherTriangle(int N) {
        for (int i = 1; i <= N; i++) {
            for (int j = 1; j <= i; j++) {
                System.out.print(j + " ");
            }
            System.out.println();
        }
    }

    public static void main(String[] args) {
        int N = Integer.parseInt(args[0]);
        afficherTriangle(N);
    }
}